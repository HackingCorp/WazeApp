{
  "name": "wizeapp",
  "version": "1.0.0",
  "description": "Production-ready WhatsApp AI Agents SaaS Platform",
  "author": "WizeApp Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:simple": "nest start --watch --entryFile main-simple",
    "start:standalone": "nest start --watch --entryFile main-standalone",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/data-source.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d src/database/data-source.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d src/database/data-source.ts",
    "ollama:install": "ollama pull llava:latest",
    "ollama:install:light": "ollama pull llava:7b",
    "ollama:list": "ollama list",
    "ollama:serve": "ollama serve",
    "vision:test": "curl http://localhost:3100/api/v1/whatsapp/vision/status"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@google-cloud/speech": "^6.4.0",
    "@google-cloud/vision": "^4.0.4",
    "@hapi/boom": "^10.0.1",
    "@mistralai/mistralai": "^1.7.5",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.1",
